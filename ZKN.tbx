      if($ZettelString){
         $ZettelString=$ZettelString+vDelim+$SiblingOrder(X).format(0,vPad,&quot;0&quot;);
      }else{
         $ZettelString=$SiblingOrder(X).format(0,vPad,&quot;0&quot;);
